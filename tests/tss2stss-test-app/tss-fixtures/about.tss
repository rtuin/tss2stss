"#aboutScroll": {
    width: Ti.UI.FILL,
    contentWidth: '100%',
    layout: 'vertical',
    top: 5,
    bottom: 5,
    left: 5,
    right: 5
},
"#drawer[platform=ios]": {
    top: 5,
    left: 5,
    right: 5,
    bottom: 5,
    width: Ti.UI.FILL,
    height: Ti.UI.SIZE
},
"#nameLabel[formFactor=handheld]": {
    height: Ti.UI.SIZE,
    layout: 'horizontal'
},
".container[platform=ios formFactor=handheld]": {
    height: Ti.UI.SIZE,
    layout: 'horizontal'
},
"#crewView[platform=ios][formFactor=handheld]": {
    height: Ti.UI.SIZE,
    layout: 'horizontal'
},
"ImageView": {
    borderRadius: 5
},
".header": {
    top: 5,
    bottom: 5,
    font: {
        fontSize: 18,
        fontStyle: 'normal',
        fontWeight: 'bold'
    }
},
"a": {
    top: 5,
    bottom: 5,
    font: {
        fontSize: 16,
        fontStyle: 'normal',
        fontWeight: 'bold'
    }
},
".this-is-a-test": {
    color: "blue"
},
".this-is-a-test[formFactor=tablet]": {
    color: "red"
},
".this-is-a-test[platform=ios if=Alloy.Globals.isIos7Plus]": {
    color: "red"
}
